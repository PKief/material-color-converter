{
  "name": "material-color-converter",
  "version": "1.0.0",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview --port 4173",
    "test": "vitest --environment jsdom",
    "lint": "bunx --bun @biomejs/biome check --write ./src",
    "format": "bunx --bun @biomejs/biome format --write ./src",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false"
  },
  "dependencies": {
    "@mdi/font": "7.4.47",
    "bootstrap": "^5.3.3",
    "chroma-js": "^2.4.2",
    "pinia": "^2.1.7",
    "roboto-fontface": "*",
    "svg-color-linter": "^2.0.3",
    "vue": "^3.4.31",
    "vue-router": "^4.4.0",
    "vuetify": "^3.6.11",
    "webfontloader": "^1.6.28"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.8.3",
    "@types/chroma-js": "^2.4.4",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^20.14.9",
    "@types/webfontloader": "^1.6.38",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.5.1",
    "jsdom": "^24.1.0",
    "npm-run-all": "^4.1.5",
    "sass": "^1.77.6",
    "sass-loader": "^14.2.1",
    "typescript": "~5.5.3",
    "vite": "^5.3.3",
    "vitest": "^1.6.0",
    "vue-tsc": "^2.0.24"
  }
}
